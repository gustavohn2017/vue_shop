{"ast":null,"code":"import axios from 'axios';\nimport Footer from '@/components/Footer.vue';\nexport default {\n  data() {\n    return {\n      showMobileMenu: false,\n      cart: {\n        items: []\n      }\n    };\n  },\n  methods: {\n    isMobile() {\n      if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {\n        return true;\n      } else {\n        return false;\n      }\n    }\n  },\n  beforeCreate() {\n    this.$store.commit(\"initializeStore\");\n    const token = this.$store.state.token;\n    if (token) {\n      axios.defaults.headers.common[\"Authorization\"] = \"Token \" + token;\n    } else {\n      axios.defaults.headers.common[\"Authorization\"] = \"\";\n    }\n  },\n  mounted() {\n    this.cart = this.$store.state.cart;\n  },\n  computed: {\n    cartTotalLength() {\n      let totalLength = 0;\n      for (let i = 0; i < this.cart.items.length; i++) {\n        totalLength += this.cart.items[i].quantity;\n      }\n      return totalLength;\n    }\n  },\n  components: {\n    Footer\n  }\n};","map":{"version":3,"sources":["C:\\Users\\Gustavo\\Desktop\\Portifólio\\ecommerce\\virtue_vue\\src\\App.vue"],"names":[],"mappings":"AA+EA,OAAO,KAAI,MAAO,OAAM;AACxB,OAAO,MAAK,MAAO,yBAAwB;AAE3C,eAAe;EACX,IAAI,GAAG;IACH,OAAO;MACH,cAAc,EAAE,KAAK;MACrB,IAAI,EAAE;QACF,KAAK,EAAE;MACX;IACJ,CAAC;EACL,CAAC;EACD,OAAO,EAAE;IACL,QAAQ,GAAG;MACP,IAAI,gEAAgE,CAAC,IAAI,CAAC,SAAS,CAAC,SAAS,CAAC,EAAE;QAC5F,OAAO,IAAI;MACf,CAAA,MACK;QACD,OAAO,KAAK;MAChB;IACJ;EACJ,CAAC;EACD,YAAY,GAAG;IACX,IAAI,CAAC,MAAM,CAAC,MAAM,CAAC,iBAAiB,CAAC;IACrC,MAAM,KAAI,GAAI,IAAI,CAAC,MAAM,CAAC,KAAK,CAAC,KAAK;IACrC,IAAI,KAAK,EAAE;MACP,KAAK,CAAC,QAAQ,CAAC,OAAO,CAAC,MAAM,CAAC,eAAe,CAAA,GAAI,QAAO,GAAI,KAAK;IACrE,CAAA,MACK;MACD,KAAK,CAAC,QAAQ,CAAC,OAAO,CAAC,MAAM,CAAC,eAAe,CAAA,GAAI,EAAE;IACvD;EACJ,CAAC;EACD,OAAO,GAAG;IACN,IAAI,CAAC,IAAG,GAAI,IAAI,CAAC,MAAM,CAAC,KAAK,CAAC,IAAI;EACtC,CAAC;EACD,QAAQ,EAAE;IACN,eAAe,GAAG;MACd,IAAI,WAAU,GAAI,CAAC;MACnB,KAAK,IAAI,CAAA,GAA