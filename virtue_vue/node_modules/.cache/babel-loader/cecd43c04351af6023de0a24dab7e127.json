{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'LogIn',\n  data() {\n    return {\n      username: '',\n      password: '',\n      errors: []\n    };\n  },\n  mounted() {\n    document.title = 'Logar | Virtue';\n  },\n  methods: {\n    async submitForm() {\n      axios.defaults.headers.common[\"Authorization\"] = \"\";\n      localStorage.removeItem(\"token\");\n      const formData = {\n        username: this.username,\n        password: this.password\n      };\n      await axios.post(\"/api/v1/token/login/\", formData).then(response => {\n        const token = response.data.auth_token;\n        this.$store.commit('setToken', token);\n        axios.defaults.headers.common[\"Authorization\"] = \"Token \" + token;\n        localStorage.setItem(\"token\", token);\n        const toPath = this.$route.query.to || '/cart';\n        this.$router.push(toPath);\n      }).catch(error => {\n        if (error.response) {\n          for (const property in error.response.data) {\n            this.errors.push(`${property}: ${error.response.data[property]}`);\n          }\n        } else {\n          this.errors.push('Something went wrong. Please try again');\n          console.log(JSON.stringify(error));\n        }\n      });\n    }\n  }\n};","map":{"version":3,"sources":["C:\\Users\\Gustavo\\Desktop\\Portifólio\\ecommerce\\virtue_vue\\src\\views\\LogIn.vue"],"names":[],"mappings":"AAyCA,OAAO,KAAI,MAAO,OAAM;AAExB,eAAe;EACX,IAAI,EAAE,OAAO;EACb,IAAI,GAAG;IACH,OAAO;MACH,QAAQ,EAAE,EAAE;MACZ,QAAQ,EAAE,EAAE;MACZ,MAAM,EAAE;IACZ,CAAA;EACJ,CAAC;EACD,OAAO,GAAG;IACN,QAAQ,CAAC,KAAI,GAAI,gBAAe;EACpC,CAAC;EACD,OAAO,EAAE;IACL,MAAM,UAAU,GAAG;MACf,KAAK,CAAC,QAAQ,CAAC,OAAO,CAAC,MAAM,CAAC,eAAe,CAAA,GAAI,EAAC;MAElD,YAAY,CAAC,UAAU,CAAC,OAAO,CAAA;MAE/B,MAAM,QAAO,GAAI;QACb,QAAQ,EAAE,IAAI,CAAC,QAAQ;QACvB,QAAQ,EAAE,IAAI,CAAC;MACnB,CAAA;MAEA,MAAM,KAAI,CACL,IAAI,CAAC,sBAAsB,EAAE,QAAQ,CAAA,CACrC,IAAI,CAAC,QAAO,IAAK;QACd,MAAM,KAAI,GAAI,QAAQ,CAAC,IAAI,CAAC,UAAS;QAErC,IAAI,CAAC,MAAM,C